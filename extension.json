{
  "manifest_version": 2,
  "name": "SesMailer",
  "type": "other",
  "author": "Janos Ronkay",
  "url": "https://github.com/hangya/mw-ses-mailer",
  "descriptionmsg": "Use AWS SES API to send emails",
  "license-name": "GPL-2.0+",
  "version": "1.1",
  "AutoloadClasses": {
    "SesMailer": "SesMailer_body.php"
  },
  "Hooks": {
    "AlternateUserMailer": [
      "SesMailer::wfSesMailer"
    ]
  },
  "config": {
    "SesMailerRegion": "eu-central-1",
    "SesMailerKey": "",
    "SesMailerSecret": ""
  }
}
